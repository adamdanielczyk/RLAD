{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": [
        "androidx.compose.compiler:compiler"
      ],
      "matchPackagePatterns": [
        "^org\\.jetbrains\\.kotlin[.:]"
      ],
      "groupName": "Kotlin / Compose Compiler"
    }
  ],
  "extends": [
    "config:base",
    ":automergeAll",
    ":automergeBranch",
    ":prConcurrentLimitNone",
    ":prHourlyLimitNone"
  ]
}
